{
    "name": "czim/aalberts-pxlcms",
    "description": "Laravel adapter for the Aalberts PXL CMS",
    "keywords": [
        "laravel",
        "eloquent",
        "database",
        "cms"
    ],
    "homepage": "https://github.com/czim/aalberts-pxlcms",
    "license": "MIT",
    "authors": [
        {
            "name": "Coen Zimmerman",
            "email": "coen@pixelindustries.com",
            "homepage": "http://www.pixelindustries.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php" : ">=5.4.0",
        "illuminate/support": "5.*",
        "illuminate/database": "5.*",
        "czim/laravel-pxlcms": "^1.0",
        "czim/laravel-listify": "^0.9",
        "laracasts/presenter": "^0.2"
    },
    "require-dev": {
        "phpunit/phpunit" : "4.*",
        "scrutinizer/ocular": "~1.1",
        "orchestra/testbench": "~3.0"
    },
    "autoload": {
        "psr-4": {
            "Aalberts\\": "src"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "scripts": {
        "test": "phpunit"
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
